    <add key="InstrumentName" value="default" />
    <add key="QuartzAutomationUsername" value="devel" />
    <add key="QuartzAutomationPassword" value="latigid56" />
    <add key="UseBootstrapStyling" value="true" />
    <add key="TakeScreenshots" value="false" />
    <add key="SuperSpeed" value="false" />
    <add key="EnableTrace" value="false" />
    <add key="Browser" value="chrome" />
