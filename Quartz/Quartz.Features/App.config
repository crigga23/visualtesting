  </configSections>
  <specFlow>
    <unitTestProvider name="MsTest" />
  </specFlow>
  <appSettings>
    <add key="Instrument" value="Osprey" />
    <add key="DefaultWait" value="1" />
    <add key="UseSimulatedInstrument" value="true" />
    <add key="TyphoonMsiInstallerLocation" value="\\Tu-server-build\Typhoon\Development\InstallersML" />
    <add key="QuartzReportLocation" value="\\TestResults\\Reports\\" />
    <add key="QuartzServerUrl" value="http://localhost" />
